{
  "name": "gnome-gestures",
  "version": "1.0.0",
  "description": "Touchpad Gesture Improvements",
  "main": "extension.js",
  "type": "module",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build:transpile": "tsc scripts/transpile.ts --outDir build/scripts --allowSyntheticDefaultImports --sourceMap --target esnext --module esnext --moduleResolution node && tsc",
    "lint:scripts": "eslint scripts",
    "lint:extension": "eslint extension",
    "lint:package": "eslint build --fix",
    "prebuild": "npm run clean && npm run lint:scripts && npm run lint:extension",
    "build": "npm run build:transpile && node build/scripts/transpile.js",
    "postbuild": "npm run lint:package && make pack",
    "clean": "rm -rf build && mkdir build",
    "update": "npm run build && make update"
  },
  "repository": {
    "type": "git",
    "url": "https://gitlab.gnome.org/harshadgavali/gnome-gestures.git"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@gi-types/clutter": "^7.0.6",
    "@gi-types/gio": "^2.66.9",
    "@gi-types/gobject": "^2.66.9",
    "@gi-types/gtk": "^4.0.9",
    "@gi-types/meta": "^3.38.5",
    "@gi-types/shell": "^0.1.6",
    "@gi-types/st": "^1.0.6",
    "@types/glob": "^7.1.4",
    "@types/node": "^16.7.13",
    "@typescript-eslint/eslint-plugin": "^4.31.0",
    "@typescript-eslint/parser": "^4.31.0",
    "eslint": "^7.32.0",
    "fast-xml-parser": "^3.19.0",
    "fs": "0.0.1-security",
    "glob": "^7.1.7",
    "path": "^0.12.7",
    "prettier": "^2.3.2",
    "typescript": "^4.4.2"
  }
}
